    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.12.0",
    "react-router-hash-link": "^2.4.3",
    "styled-components": "^6.2.0",
    "uuid": "^13.0.0",
    "yup": "^1.7.1",
    "zustand": "^5.0.9"
